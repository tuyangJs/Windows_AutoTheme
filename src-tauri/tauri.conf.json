{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "WindowsThemeAuto",
  "version": "1.2.2",
  "identifier": "com.tauri-app.cn",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "系统主题自适应",
        "label": "main",
        "windowEffects": {
          "color": "#00000000",
          "effects": [
            "acrylic"
          ]
        },
        "transparent": true,
        "center": true,
        "decorations": false,
        "maximized": false,
        "resizable": true,
        "maxHeight": 736,
        "maxWidth": 552,
        "minHeight": 572,
        "minWidth": 382,
        "width": 402,
        "height": 582
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "autostart": null
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "copyright": "Copyright © 2025 Tuyang All",
    "icon": [
      "src-tauri/icons/logo.png"
    ]
  }
}